( uxnmin tak.rom z y x )

|10 @Console/vector $2 &read $5 &type $1 &write $1 &error $1
|100

@on-reset ( -> )
	;on-console .Console/vector DEO2
	#00 BRK

@on-console ( -> )
	.Console/type DEI #02 NEQk ?{
		POP2 #0a MUL .Console/read DEI #30 SUB ADD BRK }
	INC NEQk ?{ NEQ BRK }
	POP2 tak #0180 #0e DEO2 BRK

@tak ( z y x -- res )
	LTHk ?{ POP2 JMP2r }
	ROT ROTk ROTk
	( x-1 ) #01 SUB tak STH
	( y-1 ) #01 SUB tak STH
	( z-1 ) #01 SUB tak STHr STHr !tak
